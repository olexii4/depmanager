{
  "folder": ".",
  "license": "LICENSE-HEADER.txt",
  "exact_paths_method": "EXCLUDE",
  "exact_paths": [
    "node_modules",
    "dist",
    "coverage",
    ".git",
    ".github",
    "description",
    "verify-tests.js",
    "*.json",
    "*.md",
    "*.txt",
    "LICENSE",
    ".gitignore",
    "package-lock.json",
    "tsconfig.json"
  ],
  "file_type_method": "INCLUDE",
  "file_types": [
    ".js",
    ".ts"
  ],
  "insert_license": true,
  "clear_license": false,
  "license_formats": {
    "js|ts": {
      "prepend": "/**",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    }
  }
}
